{
    "version": 2,
    "rewrites": [{ "source": "/(.*)", "destination": "src/index.ts" }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "build/index.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}
